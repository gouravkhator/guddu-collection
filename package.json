    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0"
  },
  "watch": {
    "serve": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "js,jsx"
    }
